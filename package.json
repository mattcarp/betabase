{
  "name": "siam",
  "version": "0.4.51",
  "private": true,
  "scripts": {
    "dev": "npx kill-port 3000 2>/dev/null || true && next dev -p 3000",
    "diagnose:aoma": "tsx scripts/diagnose-aoma-performance.ts",
    "test:performance": "playwright test tests/performance --reporter=list",
    "prebuild": "node scripts/generate-build-info.js || echo 'Build info script not found, skipping'",
    "build": "next build",
    "start": "next start -p $PORT",
    "start:prod": "NODE_ENV=production next start -p $PORT",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:local": "npx playwright test --project=e2e-local",
    "test:e2e:render": "playwright test --config=playwright.config.render.ts",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:smoke": "playwright test --grep @smoke",
    "test:regression": "playwright test --grep @regression",
    "test:changed": "jest -o",
    "test:feature": "playwright test --grep",
    "test:analyze": "./scripts/analyze-test-suite.sh",
    "test:cleanup": "./scripts/cleanup-test-suite.sh",
    "test:preserve": "./scripts/preserve-passing-tests.sh",
    "test:unit": "npx playwright test --project=unit",
    "test:integration": "npx playwright test --project=integration",
    "test:e2e:production": "npx playwright test --project=e2e-production",
    "test:critical": "npx playwright test tests/e2e/critical-paths/",
    "test:clean": "npx playwright test --config=playwright.config.clean.ts",
    "test:foundation": "npx playwright test tests/foundation/",
    "test:health": "npx playwright test --grep='health|smoke'",
    "test:report": "npx playwright show-report",
    "test:debug": "npx playwright test --debug",
    "test:ui": "npx playwright test --ui",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "deploy": "./scripts/deploy-rock-solid.sh",
    "deploy:validate": "./scripts/validate-before-deploy.sh",
    "deploy:quick": "./scripts/deploy-bulletproof.sh --skip-tests",
    "deploy:local-first": "npm run test:e2e:local && npm run deploy",
    "deploy:status": "./scripts/deployment-status.sh",
    "deploy:monitor": "./scripts/deploy-with-monitoring.sh"
  },
