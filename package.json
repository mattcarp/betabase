    "zod": "^3.23.8",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.0.0",
    "@playwright/test": "^1.54.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.14",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "cli-table3": "^0.6.5",
    "dotenv": "^17.2.3",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.1.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
