{
  "name": "siam",
  "version": "0.11.1",
  "private": true,
  "scripts": {
    "dev": "npx kill-port 3000 2>/dev/null || true && next dev -p 3000",
    "diagnose:aoma": "tsx scripts/diagnose-aoma-performance.ts",
    "scrape:jira": "node scripts/data-collection/scrape-jira.js",
    "scrape:jira:headless": "node scripts/data-collection/scrape-jira.js --headless",
    "update:all-data": "./scripts/data-collection/update-all-data.sh",
    "update:all-data:headless": "./scripts/data-collection/update-all-data.sh --headless",
    "test:performance": "playwright test tests/performance --reporter=list",
    "prebuild": "node scripts/generate-build-info.js || echo 'Build info script not found, skipping'",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start -p $PORT",
    "start:prod": "NODE_ENV=production next start -p $PORT",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:check": "npm run format:check && npm run lint",
    "lint:quick": "./scripts/quick-lint-check.sh",
    "lint:file": "./scripts/lint-file.sh",
    "lint:fix-all": "npm run lint:fix && npm run format",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:local": "npx playwright test --project=e2e-local",
    "test:e2e:render": "playwright test --config=playwright.config.render.ts",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:smoke": "playwright test --grep @smoke",
    "test:regression": "playwright test --grep @regression",
    "test:changed": "vitest run --changed",
    "test:feature": "playwright test --grep",
    "test:analyze": "./scripts/analyze-test-suite.sh",
    "test:cleanup": "./scripts/cleanup-test-suite.sh",
    "test:preserve": "./scripts/preserve-passing-tests.sh",
    "test:unit": "npx playwright test --project=unit",
    "test:integration": "npx playwright test --project=integration",
    "test:e2e:production": "npx playwright test --project=e2e-production",
    "test:critical": "npx playwright test tests/e2e/critical-paths/",
    "test:clean": "npx playwright test --config=playwright.config.clean.ts",
    "test:foundation": "npx playwright test tests/foundation/",
    "test:health": "npx playwright test --grep='health|smoke'",
    "test:report": "npx playwright show-report",
    "test:debug": "npx playwright test --debug",
    "test:ui": "npx playwright test --ui",
    "test:aoma": "npx playwright test tests/production/aoma-knowledge-validation.spec.ts tests/production/aoma-anti-hallucination.spec.ts --reporter=list",
    "test:aoma:knowledge": "npx playwright test tests/production/aoma-knowledge-validation.spec.ts --reporter=list",
    "test:aoma:hallucination": "npx playwright test tests/production/aoma-anti-hallucination.spec.ts --reporter=list",
    "test:aoma:chat": "npx playwright test tests/production/aoma-chat-test.spec.ts --reporter=list",
    "test:aoma:errors": "npx playwright test tests/production/aoma-error-handling.spec.ts --reporter=list",
    "test:aoma:all": "npx playwright test tests/production/aoma-*.spec.ts --reporter=list",
    "test:visual": "npx playwright test tests/visual/ --reporter=list",
    "test:visual:mac": "npx playwright test tests/visual/mac-design-system-regression.spec.ts --reporter=list",
    "test:visual:dark-theme": "npx playwright test tests/visual/dark-theme-regression.spec.ts --reporter=list",
    "test:visual:update-snapshots": "npx playwright test tests/visual/ --update-snapshots",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "pre-pr-check": "npm run format:check && npm run lint && git diff --check",
    "prepare": "husky install || true",
    "analyze": "ANALYZE=true npm run build",
    "analyze:server": "ANALYZE=true BUNDLE_ANALYZE=server npm run build",
    "analyze:browser": "ANALYZE=true BUNDLE_ANALYZE=browser npm run build",
    "deploy": "./scripts/deploy-rock-solid.sh",
    "deploy:validate": "./scripts/validate-before-deploy.sh",
    "deploy:quick": "./scripts/deploy-bulletproof.sh --skip-tests",
    "deploy:local-first": "npm run test:e2e:local && npm run deploy",
    "deploy:status": "./scripts/deployment-status.sh",
    "deploy:monitor": "./scripts/deploy-with-monitoring.sh",
    "analyze:bundle": "./scripts/analyze-bundle.sh",
    "analyze:deps": "depcheck",
    "mac:check": "node scripts/validate-mac-compliance.js",
    "mac:check-all": "find src app -type f \\( -name '*.ts' -o -name '*.tsx' -o -name '*.js' -o -name '*.jsx' -o -name '*.css' \\) -exec node scripts/validate-mac-compliance.js {} +"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "node scripts/validate-mac-compliance.js",
      "eslint --fix --max-warnings=0",
      "prettier --write"
    ],
    "*.css": [
      "node scripts/validate-mac-compliance.js",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.42",
