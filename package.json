{
  "name": "siam",
  "version": "0.24.36",
  "private": true,
  "scripts": {
    "dev": "lsof -ti:3000 | xargs kill -9 2>/dev/null || true && next dev --webpack -p 3000",
    "diagnose:aoma": "tsx scripts/diagnose-aoma-performance.ts",
    "scrape:jira": "node scripts/data-collection/scrape-jira.js",
    "scrape:jira:headless": "node scripts/data-collection/scrape-jira.js --headless",
    "update:all-data": "./scripts/data-collection/update-all-data.sh",
    "update:all-data:headless": "./scripts/data-collection/update-all-data.sh --headless",
    "test:performance": "./tests/performance/run-all-performance-tests.sh",
    "test:performance:quick": "playwright test tests/performance/chat-response-time.spec.ts --reporter=list",
    "test:performance:vitals": "playwright test tests/performance/web-vitals-chat.spec.ts --reporter=list",
    "prebuild": "node scripts/generate-build-info.js || echo 'Build info script not found, skipping'",
    "build": "next build --webpack",
    "build:analyze": "ANALYZE=true next build --webpack",
    "start": "next start -p $PORT",
    "start:prod": "NODE_ENV=production next start -p $PORT",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "lint:check": "npm run format:check && npm run lint",
    "lint:quick": "./scripts/quick-lint-check.sh",
    "lint:file": "./scripts/lint-file.sh",
    "lint:fix-all": "npm run lint:fix && npm run format",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run --exclude tests/integration/**",
    "test:integration": "INTEGRATION_TESTS=1 vitest run tests/integration/**",
    "deduplicate": "./scripts/doc-analysis/deduplicate.sh",
    "deduplicate:dry-run": "./scripts/doc-analysis/deduplicate.sh --dry-run",
    "test:integration:with-server": "./scripts/test-with-server.sh",
    "test:all": "./tests/run-all-tests.sh",
    "test:ui-all": "./tests/run-ui-tests.sh",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:local": "PLAYWRIGHT_BASE_URL=http://localhost:3000 npx playwright test --project=e2e-local",
    "test:e2e:production": "npx playwright test --project=e2e-production",
    "test:changed": "vitest run --changed",
    "test:report": "npx playwright show-report",
    "test:smoke": "npx playwright test tests/e2e/smoke/ --reporter=list",
    "test:smoke:local": "PLAYWRIGHT_BASE_URL=http://localhost:3000 npx playwright test tests/e2e/smoke/ --reporter=list",
    "test:features": "npx playwright test tests/e2e/features/ --reporter=list",
    "test:features:local": "PLAYWRIGHT_BASE_URL=http://localhost:3000 npx playwright test tests/e2e/features/ --reporter=list",
    "test:critical": "npx playwright test tests/e2e/critical-paths/ --reporter=list",
    "test:demo": "npx playwright test tests/e2e/demo/ --reporter=list",
    "test:aoma": "npx playwright test tests/e2e/production/siam-aoma-knowledge-validation.spec.ts tests/e2e/production/siam-aoma-anti-hallucination.spec.ts --reporter=list",
    "test:aoma:knowledge": "npx playwright test tests/e2e/production/siam-aoma-knowledge-validation.spec.ts --reporter=list",
    "test:aoma:hallucination": "npx playwright test tests/e2e/production/siam-aoma-anti-hallucination.spec.ts --reporter=list",
    "test:aoma:all": "npx playwright test tests/e2e/production/siam-aoma-*.spec.ts --reporter=list",
    "test:prod": "npx playwright test tests/e2e/production/ --reporter=list",
    "test:visual": "npx playwright test tests/e2e/visual/ --reporter=list",
    "test:visual:local": "PLAYWRIGHT_BASE_URL=http://localhost:3000 npx playwright test tests/e2e/visual/ --reporter=list",
    "test:visual:mac": "npx playwright test tests/e2e/visual/mac-design-system-regression.spec.ts --reporter=list",
    "test:visual:dark-theme": "npx playwright test tests/e2e/visual/dark-theme-regression.spec.ts --reporter=list",
    "test:visual:update-snapshots": "npx playwright test tests/e2e/visual/ --update-snapshots",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "pre-pr-check": "npm run format:check && npm run lint && git diff --check",
    "prepare": "husky install || true",
    "analyze": "ANALYZE=true npm run build",
    "analyze:server": "ANALYZE=true BUNDLE_ANALYZE=server npm run build",
    "analyze:browser": "ANALYZE=true BUNDLE_ANALYZE=browser npm run build",
    "deploy": "./scripts/deploy-rock-solid.sh",
    "deploy:validate": "./scripts/validate-before-deploy.sh",
    "deploy:quick": "./scripts/deploy-bulletproof.sh --skip-tests",
    "deploy:local-first": "npm run test:e2e:local && npm run deploy",
    "deploy:status": "./scripts/deployment-status.sh",
    "deploy:monitor": "./scripts/deploy-with-monitoring.sh",
    "analyze:bundle": "./scripts/analyze-bundle.sh",
    "analyze:deps": "depcheck",
    "mac:check": "node scripts/validate-mac-compliance.js",
    "mac:check-all": "find src app -type f \\( -name '*.ts' -o -name '*.tsx' -o -name '*.js' -o -name '*.jsx' -o -name '*.css' \\) -exec node scripts/validate-mac-compliance.js {} +",
    "aoma:download-docs": "node scripts/aoma/download-bottom-docs.js",
    "aoma:download-docs:manual": "AOMA_MANUAL_CAPTURE=1 node scripts/aoma/download-bottom-docs.js",
    "aoma:crawl:auth": "node scripts/aoma/firecrawl-auth-crawl.mjs",
    "crawl:v25": "node scripts/aoma/firecrawl-v2.5-crawl.mjs",
    "crawl:pw": "node scripts/aoma/playwright-full-crawl.mjs",
    "ingest:aoma": "node scripts/aoma/ingest-crawled-to-supabase.mjs",
    "ingest:aoma:dry-run": "node scripts/aoma/ingest-crawled-to-supabase.mjs --dry-run"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "node scripts/validate-mac-compliance.js",
      "eslint --fix --max-warnings=0",
      "prettier --write"
    ],
    "*.css": [
      "node scripts/validate-mac-compliance.js",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
