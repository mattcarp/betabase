{
  "mcpServers": {
    "playwright-mcp": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@executeautomation/playwright-mcp-server"],
      "env": {
        "NODE_ENV": "development",
        "PLAYWRIGHT_OUTPUT_DIR": "./screenshots"
      }
    },
    "aoma-mesh": {
      "type": "stdio",
      "command": "node",
      "args": ["/Users/matt/Documents/projects/aoma-mesh-mcp/dist/aoma-mesh-server.js"],
      "env": {
        "NODE_ENV": "production"
      }
    },
    "ref-tools": {
      "type": "http",
      "url": "https://api.ref.tools/mcp?apiKey=ref-851444f482b0cdffec41"
    },
    "task-master-ai": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "--package=task-master-ai", "task-master-ai"],
      "env": {
        "ANTHROPIC_API_KEY": "YOUR_ANTHROPIC_API_KEY_HERE",
        "PERPLEXITY_API_KEY": "YOUR_PERPLEXITY_API_KEY_HERE",
        "OPENAI_API_KEY": "YOUR_OPENAI_KEY_HERE",
        "GOOGLE_API_KEY": "YOUR_GOOGLE_KEY_HERE",
        "XAI_API_KEY": "YOUR_XAI_KEY_HERE",
        "OPENROUTER_API_KEY": "YOUR_OPENROUTER_KEY_HERE",
        "MISTRAL_API_KEY": "YOUR_MISTRAL_KEY_HERE",
        "AZURE_OPENAI_API_KEY": "YOUR_AZURE_KEY_HERE",
        "OLLAMA_API_KEY": "YOUR_OLLAMA_API_KEY_HERE"
      }
    },
    "semgrep": {
      "type": "stdio",
      "command": "semgrep-mcp",
      "args": ["-t", "stdio"],
      "env": {
        "NODE_ENV": "development"
      }
    },
    "supabase": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "supabase-mcp"],
      "env": {
        "SUPABASE_URL": "https://kfxetwuuzljhybfgmpuc.supabase.co",
        "SUPABASE_SERVICE_ROLE_KEY": "${SUPABASE_SERVICE_ROLE_KEY}"
      }
    },
    "render": {
      "type": "http",
      "url": "https://mcp.render.com/mcp",
      "headers": {
        "Authorization": "Bearer ${RENDER_API_KEY}"
      }
    },
    "chrome-devtools": {
      "type": "stdio",
      "command": "node",
      "args": ["./node_modules/chrome-devtools-mcp/build/src/index.js"]
    },
    "shadcn": {
      "type": "stdio",
      "command": "npx",
      "args": ["shadcn@latest", "mcp"]
    }
  }
}
